export TERM=xterm-256color
export EDITOR=vim

export PATH=$HOME/.local/bin:/usr/local/bin:/usr/sbin:/sbin/:$PATH

COLORONE="\[\e[1;32m\]"
COLORTWO="\[\e[0;34m\]"
COLORTHREE="\[\e[0;32m\]"
COLORFOUR="\[\e[0;37m\]"

export PS1="${COLORONE}\u@\h ${COLORTWO}\w ${COLORTHREE}\$ ${COLORFOUR}"
